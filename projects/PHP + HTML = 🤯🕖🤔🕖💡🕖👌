<?php
$array = ['one', 'two', 'three', 'four', 'five'];
$headline = 'Final Countdown';
function generatelist(array $array, $headline, ???) : string
{
    $html = "<h3> $headline </h3>";
    $html .= '<ol>';
    $list = array_values($array);
foreach ($list as $countdown)
{
    $html .= "<li> $countdown </li>";
}
$html .= '</ol>';
return $html;
}
?>
<! doctype html
<html lang="en">
<head>
<title> Функция для генерации html списков </title>
</head>
    <body>
   <?= generatelist($array, $headline, ???) ?>
</body>
</html>
